{
  "name": "videoprompter",
  "version": "0.1.0",
  "private": true,
  "description": "고성능 영상 기획 및 생성 플랫폼",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:smoke": "cypress run --spec 'cypress/e2e/smoke/**/*'",
    "test:a11y": "cypress run --spec 'cypress/e2e/accessibility/**/*'",
    "check:circular-deps": "madge --circular --extensions ts,tsx src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-all": "pnpm typecheck && pnpm lint && pnpm format:check && pnpm test && pnpm test:e2e:smoke"
  },
