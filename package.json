    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^3.0.6",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.544.0",
