{
  "name": "videoplanet",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e:dev": "PW_BASE_URL=http://localhost:3000 playwright test",
    "test:e2e:ui": "PW_BASE_URL=http://localhost:3000 playwright test --ui",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test:e2e": "playwright test",
    "test:mcp": "node scripts/test-mcp-servers.js",
    "test:mcp:enhanced": "npm test -- src/__tests__/mcp-enhanced-testing.test.ts",
    "test:mcp:integration": "npm test -- src/__tests__/mcp-real-integration.test.ts",
    "test:mcp:website": "npm test -- src/__tests__/mcp-real-website.test.ts",
    "test:mcp:performance": "npm test -- src/__tests__/mcp-performance.test.ts",
    "test:mcp:all": "./scripts/run-mcp-tests.sh",
    "test:mcp:ci": "npm run test:mcp:enhanced && npm run test:mcp:integration && npm run test:mcp:website && npm run test:mcp:performance",
    "monitor:mcp": "node scripts/mcp-performance-monitor.js monitor",
    "report:mcp": "node scripts/mcp-performance-monitor.js report",
    "notify:mcp:test": "node scripts/mcp-notification-service.js test",
    "setup:mcp:notifications": "node scripts/mcp-notification-service.js config"
  },
  "dependencies": {
    "@playwright/mcp": "^0.0.34",
    "@prisma/client": "^5.18.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "google-auth-library": "^10.2.1",
    "lucide-react": "^0.540.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "postcss": "^8.4.31",
    "prisma": "^5.18.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
