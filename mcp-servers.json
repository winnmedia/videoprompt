{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp"],
      "env": {
        "PLAYWRIGHT_BROWSERS_PATH": "0"
      }
    },
    "context7": {
      "command": "node",
      "args": ["./src/lib/mcp-servers/context7/index.mjs", "--transport", "stdio"]
    },
    "sequential-thinking": {
      "command": "node",
      "args": ["./src/lib/mcp-servers/sequential-thinking/index.mjs", "--transport", "stdio"]
    }
  }
}
